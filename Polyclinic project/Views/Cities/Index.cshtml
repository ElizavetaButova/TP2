@model IEnumerable<Polyclinic_project.Models.Cities>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.City_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.City_code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.City_code }) |
            @Html.ActionLink("Details", "Details", new { id = item.City_code }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.City_code })
        </td>
    </tr>
}

</table>
